{
    "builds": [
      {
        "src": "server/api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "api/*.ts": {
        "runtime": "nodejs18.x"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/api/$1.ts"
      }
    ]
  }
  