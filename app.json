{
  "expo": {
    "name": "Spender",
    "slug": "Spender",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.png",
    "scheme": "spender",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/Icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Theme.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "האפליקציה צריכה גישה לתמונות שלך כדי לבחור תמונת פרופיל.",
          "cameraPermission": "האפליקציה צריכה גישה למצלמה שלך כדי לצלם תמונת פרופיל."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "splash": {
      "image": "./assets/images/Theme.png",
      "resizeMode": "contain",
      "backgroundColor": "#471396"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e3a12fd3-502a-4fc2-8b29-47468781c59b"
      }
    },
    "owner": "matan1ohayon",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e3a12fd3-502a-4fc2-8b29-47468781c59b"
    }
  }
}
